     <script>
      function calc() {
          var n1 = parseFloat(document.getElementById('n1').value);
          var n2 = parseFloat(document.getElementById('n2').value);
          var n3 = parseFloat(document.getElementById('n3').value);
    
          var meth = document.getElementById('meth').value;
          if (typeof n1 !== 'undefined' && typeof n3 !== 'undefined' && n1 <= 1600 && n1 >= 400 && n3 <= 40 && n3 >= 20) {
              if (meth === '1') {
                  if (n1 >= 1050) {
                      if (typeof n2 !== 'undefined' && n2 >= 1100) {
                          if (n1 >= 1090) {
                              document.getElementById('result').value = n1 * 69 / 1600 + n2 * 15 / 1600 + n3;
                          } else {
                              document.getElementById('result').value = n1 * 60 / 1600 + n2 * 15 / 1600 + n3;
                          }
                      } else {
                          var n2 = 0;
                          if (n1 >= 1090) {
                              document.getElementById('result').value = n1 * 69 / 1600 + n2 * 15 / 1600 + n3;
                          } else {
                              document.getElementById('result').value = n1 * 60 / 1600 + n2 * 15 / 1600 + n3;
                          }
                      }
                  } else {
                      document.getElementById('result').value = "You must get at least 1050 score";
                  }
              }
    
              if (meth === '2') {
                  if (typeof n2 !== 'undefined' && n2 >= 900) {
                      if (n1 >= 1090) {
                          document.getElementById('result').value = n1 * 75 / 1600 + n2 * 15 / 1600 + n3;
                      } else {
                          document.getElementById('result').value = n1 * 60 / 1600 + n2 * 15 / 1600 + n3;
                      }
                  } else {
                      var n2 = 0;
                      if (n1 >= 1090) {
                          document.getElementById('result').value = n1 * 75 / 1600 + n2 * 15 / 1600 + n3;
                      } else {
                          document.getElementById('result').value = n1 * 60 / 1600 + n2 * 15 / 1600 + n3;
                      }
                  }
              }
    
              if (meth === '3') {
                  if (n1 >= 890) {
                      if (typeof n2 !== 'undefined' && n2 >= 900) {
                          if (n1 >= 1090) {
                              document.getElementById('result').value = n1 * 69 / 1600 + n2 * 15 / 1600 + n3;
                          } else {
                              document.getElementById('result').value = n1 * 60 / 1600 + n2 * 15 / 1600 + n3;
                          }
                      } else {
                          var n2 = 0;
                          if (n1 >= 1090) {
                              document.getElementById('result').value = n1 * 69 / 1600 + n2 * 15 / 1600 + n3;
                          } else {
                              document.getElementById('result').value = n1 * 60 / 1600 + n2 * 15 / 1600 + n3;
                          }
                      }
                  } else {
                      document.getElementById('result').value = "You must get at least 890 score";
                  }
    
              }
          } else {
              document.getElementById('result').value = "Enter a valid Score";
          }
      }
    </script>
    <script>
      function calco() {
          var n11 = parseFloat(document.getElementById('n11').value);
          var n22 = parseFloat(document.getElementById('n22').value);
          var n33 = parseFloat(document.getElementById('n33').value);
    
          if (typeof n11 !== 'undefined' && typeof n33 !== 'undefined' && typeof n22 !== 'undefined' && n11 <= 36 && n22 <= 36 && n33 <= 36) {
              document.getElementById('resulto').value = (((n11 + n22) / 2) + n33) / 2;
          } else {
              document.getElementById('resulto').value = "Enter a valid Score";
          }
      }
    </script>
    <script>
      function showSuitable() {
          var Exam = document.getElementById('Exam').value;
          if (Exam == '1') {
              document.getElementById('sat').style.display = 'block';
              document.getElementById('act').style.display = 'none';
          } else if (Exam == '2') {
              document.getElementById('sat').style.display = 'none';
              document.getElementById('act').style.display = 'block';
          }
      }
    </script>


